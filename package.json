{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 1764",
    "analyze": "ANALYZE=true next build",
    "build": "next build && echo 'ui.mantine.dev' > out/CNAME && touch out/.nojekyll",
    "deploy": "npm run build && gh-pages -d out -t true",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint data components lib pages",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,css}\"",
    "test": "npm run prettier:check && npm run lint && npm run typecheck",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.3.0",
    "@mantine/carousel": "7.14.1",
    "@mantine/core": "7.14.1",
    "@mantine/dropzone": "7.14.1",
    "@mantine/form": "7.14.1",
    "@mantine/hooks": "7.14.1",
    "@mantine/spotlight": "7.14.1",
    "@mantinex/dev-icons": "^1.0.1",
    "@mantinex/mantine-header": "^1.0.1",
    "@mantinex/mantine-logo": "^1.0.1",
    "@mantinex/mantine-meta": "^1.0.1",
    "@mantinex/shiki": "^1.0.2",
    "@next/bundle-analyzer": "^14.0.3",
    "@tabler/icons-react": "^2.41.0",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^7.1.0",
    "next": "14.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "shikiji": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@eslint/js": "^9.15.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/node": "^22.9.0",
    "@types/react": "18.3.12",
    "babel-loader": "^9.1.3",
    "eslint": "^9.15.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.0",
    "storybook": "^7.5.3",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "packageManager": "yarn@4.5.1"
}
